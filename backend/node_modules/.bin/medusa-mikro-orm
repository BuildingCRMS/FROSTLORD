else
  export NODE_PATH="/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/@medusajs+framework@2.8.8_@medusajs+cli@2.8.8_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mi_myunnb3doomeyxcgleh63mkw4a/node_modules/@medusajs/framework/dist/mikro-orm-cli/node_modules:/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/@medusajs+framework@2.8.8_@medusajs+cli@2.8.8_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mi_myunnb3doomeyxcgleh63mkw4a/node_modules/@medusajs/framework/dist/node_modules:/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/@medusajs+framework@2.8.8_@medusajs+cli@2.8.8_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mi_myunnb3doomeyxcgleh63mkw4a/node_modules/@medusajs/framework/node_modules:/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/@medusajs+framework@2.8.8_@medusajs+cli@2.8.8_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mi_myunnb3doomeyxcgleh63mkw4a/node_modules/@medusajs/node_modules:/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/@medusajs+framework@2.8.8_@medusajs+cli@2.8.8_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mi_myunnb3doomeyxcgleh63mkw4a/node_modules:/mnt/c/Users/lpmr4/OneDrive/Desktop/FROSTLORD/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
